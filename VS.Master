                                <td class="tablasombra" id="td_titulos_0_salir" runat="server" style="width:3%">
                                    <img id="icono_menu_salir"
                                            runat="server" 
                                            src="data:image/png;base64,"
                                            style="cursor:pointer;vertical-align:middle;width:1vw;height:auto;" 
                                            lang="TRADUCIR"
											title="Salir"
											alt="<font lang='ES'>Ir a Menú DashBoard</font>"
                                            onclick="var h=function(){window.location.href='login.aspx'}; 
                                                     var callbacks=$.Callbacks(); 
                                                     callbacks.add(h); 
                                                     callbacks.fire(); " />
                                </td>								
                                <!-- ------------- -->       
                                <!-- --- Fin ----- -->
                                <!-- ------------- -->
                            </tr>
                        </table>
                    </div>
                </div>
            </td>
        </tr>
    </table>

    <!-- ------------------------ -->       
    <!-- --- Form de Contenidos - -->
    <!-- ------------------------ -->

    <form id="fmaster"  
	      runat="server" 
          defaultbutton="btnDisableEnter" 
          style="height:94vh;width:99%;padding:0px;margin:auto;border:0px solid #f0f;overflow:auto"
          onmouseover="var ml=document.getElementsByClassName('MENULATERAL'); if (ml) {$(ml[0]).hide('slow')};"
        >

        <asp:Button ID="bt_ed_perfil" Style="display:none" runat="server" ClientIDMode="Static" Text="Edicion" />
        <asp:Button ID="bt_ok_perfil" Style="display:none" runat="server" ClientIDMode="Static" Text="Edicion" />
        <asp:Button ID="bt_ko_perfil" Style="display:none" runat="server" ClientIDMode="Static" Text="Edicion" />
        
        <ajaxtool:DragPanelExtender id="DPExt_perfil" runat="server" TargetControlID="capa_perfil" DragHandleID="dcapa_perfil"></ajaxtool:DragPanelExtender>
        <ajaxtool:modalpopupextender id="MP_perfil" runat="server"   TargetControlID="bt_ed_perfil" PopupControlID="capa_perfil" CancelControlID="bt_ko_perfil" DropShadow="false" BackgroundCssClass="FondoAplicacion" RepositionMode="None"></ajaxtool:modalpopupextender>

        <!-- ------------------------- -->       
        <!-- --- Tabla de Contenidos - -->
        <!-- ------------------------- -->

        <table id="tabla_contenidos"
             style ="border-collapse:collapse
                    ;padding        :0px
                    ;border         :0px solid #f00
                    ;width          :100%
                    ;height         :100%
                    ;margin         :auto
                    ">
            <!-- ------------------------------ -->
            <!-- -------- Contenidos ---------- -->
            <!-- ------------------------------ -->
            <tr>
                <td id="principal" 
					style="vertical-align:top;text-align:left;padding:0px;margin:auto;border:0px solid #efefef;width:100%"
                    onmouseover="var ml=document.getElementsByClassName('MENULATERAL');
                                 if (ml) {$(ml[0]).hide('slow')};
                                 var lt=document.getElementsByClassName('LOGOTIPO');
                                 if (lt) {$(lt[0]).show('slow')};"
                 >
                    <!-- -------------------------- -->
                    <!-- ----- Menú General ------- -->
                    <!-- -------------------------- -->
                    <div id="MENUGENERAL" 
                         class="MENUGENERAL"
                         runat="server" 
                         style="border:solid 1px #eee;margin:auto;padding:0px;border-bottom-width:0px;border-top-width:0px;margin-left:200px;"
                         onmouseover="dentro_menu_general='SI';"
                         onmouseout="dentro_menu_general='NO';">
                    </div>
                    <!-- -------------------------- -->
                    <!-- -- Contenidos Hijos ------ -->
                    <!-- -------------------------- -->
                    <div class="main" 
                         style="padding:0px; margin:auto;border:0px dotted #ccc;width:98%;height:98%;margin:auto;">
                        <asp:ContentPlaceHolder ID="MainContent" runat="server" />
                    </div>
                    <!-- ---------------------------------- -->
                </td>
                <!-- ---------------------------------- -->
            </tr>
        </table>
        <!-- ---------------------------------- -->
        <!-- -------- Button Form ------------- -->
        <!-- ---------------------------------- -->
        <asp:Button ID="btnDisableEnter" runat="server" Text="" OnClientClick="return false;" Style="display:none;"/>
    </form>

    <!-- ------------------------------------- -->
    <!-- Panel de Peticion de Datos del Perfil -->
    <!-- ------------------------------------- -->
	
    <div id="ocapa_perfil" class="FondoAplicacion" runat="server" style="position:absolute;width:100px;height:100px;top:-10000px;left:0px;"> </div>
    <asp:Panel id="capa_perfil" 
          runat="server" 
          ClientIDMode="Static" 
          Style="display:none" >

        <table style="width:100%;margin:auto;" >
            <tr>
                <td style="text-align:center;width:2vw">
                    <img alt="Cerrar" 
					     src="data:image/png;base64," 
 					     style="cursor:pointer;vertical-align:middle;width:1vw;height:auto;" 
						 onclick="WCancela_Pide_Perfil();" />
                </td>
                <td style="width:auto">
                    <asp:Panel id="dcapa_perfil" runat="server" ClientIDMode="Static" class="dragPanel" style="border: 1px solid #899;color: #fff;font-weight: normal;padding: 0px;margin:0px;width:100%;margin:auto;cursor:-webkit-grab;cursor: grab;" >
                        <table class="tituventana">
                            <tr>
                                <td style="width:auto;text-align:left;vertical-align:middle;margin:auto;background:black;color:white;">
                                    <a lang="TRADUCIR" id="titulo_ventana_perfil">Datos del Perfil</a>
                                </td>
                            </tr>
                        </table>
                    </asp:Panel>
                </td>
                <td style="text-align:center;width:2vw">
                    <img alt="Cerrar" 
					     src="data:image/png;base64," 
						 style="cursor:pointer;vertical-align:middle;width:1vw;height:auto;" 
						 onclick="WCancela_Pide_Perfil();" />
                </td>
            </tr>
        </table>
        <div id="datos_perfil"   style="height:95%;width:100%;border:solid 0px #ddd;background:#fff"></div>
        <div id="botones_perfil" style="margin:10% 10% 10% 10%;text-align:center"></div>
	</asp:Panel>

</body>
</html>
